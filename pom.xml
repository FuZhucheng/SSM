  <build>
    <finalName>SSM</finalName>
    <!--方便MAVEN 工程打包resources目录外的更多资源文件-->
    <resources>
      <resource>
        <directory>src/main/java</directory>
        <includes>
          <include>**/*.xml</include>
        </includes>
      </resource>
    </resources>
  </build>
</project>
